<!--pages/my/my.wxml-->
<view class="top">
    <view class="left">
        <image src="{{photo}}" mode="widthFix"/>
        <view>
            <text>账号昵称：{{token.nickName}}</text>
            <text bindtap="handelLongin">{{loginStatus}}</text>
        </view>
    </view>
    <view class="right">
        <view>
            <image src="/icon/客服.png" mode="widthFix"/>
            <text>官方客服</text>
        </view>
        <view>
            <image src="/icon/设置.png" mode="widthFix"/>
            <text>设置</text>
        </view>
    </view>
</view>