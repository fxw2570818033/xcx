/* pages/main/main.wxss */
/* swiper 有 固定高度  给swiper的高度 单位变成rpx*/
swiper{
    height:300rpx;
}
swiper image{
    width:100%;
}
.list{
    
}
.list .list_item{
    display:flex;
    padding:15rpx;
    align-items:center;
}
.list_item image{
    width:200rpx;
}
.list_item>view{
    display:flex;
    flex-direction:column;
    justify-content:space-between;
    width:500rpx;
    height:200rpx;
}
.list_item>view .feature{
    overflow:hidden;
    white-space:nowrap;
    text-overflow:ellipsis;
}